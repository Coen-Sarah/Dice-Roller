<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="rootBox" alignment="CENTER" maxHeight="400.0" maxWidth="400.0" minHeight="400.0" minWidth="400.0" prefHeight="400.0" prefWidth="400.0" spacing="20.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.diceroller.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <HBox fx:id="diceButtons" prefHeight="0.0" prefWidth="360.0" VBox.vgrow="ALWAYS">
         <children>
            <Button fx:id="alf" mnemonicParsing="false" text="Button" />
            <Button fx:id="bet" mnemonicParsing="false" text="Button" />
            <Button fx:id="gam" mnemonicParsing="false" text="Button" />
         </children>
      </HBox>
   </children>
</VBox>
